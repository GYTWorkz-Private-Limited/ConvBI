# Copy this file to .env and fill in your values

# PostgreSQL Configuration (External Database)
QUERY_DB_HOST=your_postgres_host
QUERY_DB_PORT=5432
QUERY_DB_NAME=your_database_name
QUERY_DB_USER=your_db_user
QUERY_DB_PASSWORD=your_db_password

# Azure OpenAI Configuration
AZURE_OPENAI_ENDPOINT=your_azure_openai_endpoint
AZURE_OPENAI_DEPLOYMENT_NAME=your_deployment_name
AZURE_OPENAI_API_VERSION=2024-02-15-preview
AZURE_OPENAI_API_KEY=your_azure_openai_api_key

# Redis Configuration (Auto-configured for Docker, but you can override)
REDIS_PASSWORD=
REDIS_DB=0

# Qdrant Configuration (Auto-configured for Docker, but you can override)
QDRANT_API_KEY=

# Cohere Reranking (Optional)
COHERE_API_KEY=
COHERE_RERANK_MODEL=rerank-v3.5
COHERE_RERANK_TOP_K=10

# Langfuse Observability (Optional)
LANGFUSE_PUBLIC_KEY=
LANGFUSE_SECRET_KEY=

